{
	"name": "CopyPipeline_SA_Vantage_EventHub",
	"properties": {
		"description": "Real Time Streaming Data copy from SA to Vantage",
		"activities": [
			{
				"name": "Copy_zuw",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "lr255021/weatheroutput/0_1da9aca8a68b4b32b4d453f8750977a8_1.json"
					},
					{
						"name": "Destination",
						"value": "\"AFP_2021_lr255021\".\"Stream_Agg\""
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": "device1",
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "OdbcSink",
						"writeBatchSize": 0
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"validateDataConsistency": true,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "AzureBlobStorage1",
								"type": "LinkedServiceReference"
							},
							"path": "lr255021/output"
						}
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['deviceId']"
								},
								"sink": {
									"name": "deviceId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['AVERAGE_TEMP']"
								},
								"sink": {
									"name": "AVERAGE_TEMP",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "$['AVERAGE_HUMIDITY']"
								},
								"sink": {
									"name": "AVERAGE_HUMIDITY",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "$['Time']"
								},
								"sink": {
									"name": "TimeEntered",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['RowCount']"
								},
								"sink": {
									"name": "RowCount",
									"type": "Int32"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_zuw",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_zuw",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}